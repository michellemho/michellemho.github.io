---
---
{
"type": "FeatureCollection",
"features": [
{% assign places = site.posts  %}
{% for place in places %}
{
"type": "Feature",
"properties": {
"date": "{{ place.date | date: "%B %d %Y" }}",
"past_or_future": {% if place.tags contains 'upcoming' %}"future"{% else %}"past"{% endif %},
"icon": "car",
"something else": "{{ place.title | upcase }}",
"title": "{{ place.title }}",
"excerpt": "{{ place.excerpt | escape | strip }}",
"url": "{{site.url}}{{place.url}}"
},
"geometry": {
"type": "Point",
"coordinates": [{{ place.coordinates }}]
}
}
{% if forloop.rindex > 1 %},{% endif%}{% endfor %}
]
}
